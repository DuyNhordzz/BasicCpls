#include <iostream>
#include <cmath>
#include <iomanip>
using namespace std;
int main()
{
	int x1, x2, y1, y2, a, b;
	double S;
	cout << "nhap x1,y1: ";
	cin >> x1 >> y1;
	cout << "nhap x2,y2: ";
	cin >> x2 >> y2;
	S = sqrt(pow(x2 - x1, 2) + pow(y2 - y1, 2));
	cout << fixed << setprecision(2) << S;
	return 0;
}
